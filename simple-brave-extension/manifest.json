{
  "manifest_version": 3,
  "name": "Doom Bookmarks Simple",
  "version": "1.0.0",
  "description": "Simple bookmark capture for Doom Emacs",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Bookmark Capture"
  }
}
